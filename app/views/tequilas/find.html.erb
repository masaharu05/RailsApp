<%= form_tag(controller: "tequilas", action: "find") do %>

  <div class="form-group">
    <label for="find">検索</label>
    <%= text_field_tag("find","",{class:"form-control"}) %>
  </div>
  <%= submit_tag("Click") %>
<% end %>


<table class="table">
  <tr>
  
  <th>name</th>
  <th>memo</th>
  </tr>
<% @tequila.each do |obj| %>
  <tr>
  
  <th><%= obj.name %></th>
  <th><%= obj.memo %></th>
  </tr>
<% end %>
</table>

<a href="/tequilas/">Tequila</a>